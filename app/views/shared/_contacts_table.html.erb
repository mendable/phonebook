<table>
  <tr>
    <th>Photo</th>
    <th>Name</th>
    <th>Company</th>
    <th>Email</th>
    <th>Phone</th>
  </tr>

<% contacts.each do |contact| %>
  <tr>
    <td><%= link_to image_tag(contact.photo.url(:thumb)), [contact.company, contact] %></td>
    <td><%= link_to "#{contact.firstname} #{contact.lastname}", [contact.company, contact] %></td>
    <td><%=h contact.company.name %></td>
    <td><%=h contact.email %></td>
    <td><%=h contact.phone %></td>
  </tr>
<% end %>
</table>
<br />
<%= will_paginate contacts %>

